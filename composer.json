    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "sync",
        "synchronization"
    ],
    "authors": [
        {
            "name": "Luca Di Fazio",
            "email": "highliuk@gmail.com"
        }
    ],
    "autoload": {
